{
    "XCVR":
    {
        "xcvr_present": {
            "i2c":{
                "valmap-QSFP-DD": {
                    "0": true,
                    "1": false
                },
                "valmap-SFP+": {
                    "0": true,
                    "1": false
                }
            }
        }
    },

    "PSU":
    {
        "psu_present":
        {
            "bmc":
            {
                "valmap": { "0":true, "1":false}
            }
        },
        "psu_fan_dir":
        {
            "bmc":
            {
                "valmap": { "1":"INTAKE", "0":"EXHAUST" }
            }
        },
        "psu_led_color":
        {
            "colmap": {"green":"green", "red":"red"}
        },
        "PSU_FAN_MAX_SPEED":"29000"
    },

    "FAN":
    {
        "direction":
        {
            "bmc":
            {
                "valmap": { "1":"INTAKE", "0":"EXHAUST" }
            }
        },

        "present":
        {
            "i2c":
            {
                "valmap": {"1":true, "0":false}
            },
            "bmc":
            {
               "valmap": {"0":true, "1":false}
            }
        },

        "fan_master_led_color":
        {
            "colmap": {"green":"green", "amber":"amber", "red": "red", "off": "off"}
        },
        "FAN_MAX_RPM_SPEED":
        {
            "0": {"Front": "30200", "Rear": "32000"},
            "1": {"Front": "28000", "Rear": "32000"}
        },
        "pwm_to_duty_cycle": "lambda pwm: ((pwm*100)/255)"
    }

}
